amount = int(input("Enter sale price: "))
if(amount>0):
    if amount>=200:
        dis = amount*0.20  
    if 200>amount>=100:
        dis = amount*0.15 
    if amount<=50:
        dis = amount*0.05
    if 100>amount>=50:
        dis = amount*0.10
    print("disount : ",dis)
    print("net pay : ",amount-dis)
